class Ball{
    field int x, y;
    field int radius;
    constructor Ball new()
    {
        let x = 100;
        let y = 100;
        let radius = 20;
        do draw();
        return this;
    }

    method void draw()
    {
        do Screen.setColor(true);
        do Screen.drawCircle(x, y, radius);
        return;
    }

    method void Jump()
    {
        return;
    }
    
}